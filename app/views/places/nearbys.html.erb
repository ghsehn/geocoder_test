<h2>Places near <%= @distance %> km from <%= @place.address %></h2>

<%= render 'nearby_form' %>

<% if @nearbys.any? %>
  <ul>
  <% @nearbys.each do |place| %>
    <li><%= link_to place.address, nearbys_place_path(place, distance: @distance) %></li>
  <% end %>
  </ul>
<% else %>
  <p>No results</p>
<% end %>

<%= link_to 'Back', @place %>